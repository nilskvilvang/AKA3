@article{Alemi1996,
  title = {Risk-Adjusted Control Charts for Health Care Assessment},
  author = {Alemi, Farrokh and Rom, Walter and Eisenstein, Eric},
  year = {1996},
  journal = {Annals of Operations Research},
  volume = {67},
  number = {1},
  pages = {45--60},
  issn = {1572-9338},
  doi = {10.1007/BF02187023},
  abstract = {The recent Joint Commission on Accreditation of Healthcare Organization (JCAHO) requirement that hospital accreditation be based upon a Total Quality Management (TQM) approach has focused the attention of health care administrations on the use of techniques such as control charts. However, control charts are not typically adjusted for severity of illness. This adjustment is needed because, unlike industrial organizations, hospitals are not able to control all of their inputs and must accept variances in their patients. In this paper, we present a methodology for adjusting a health care organization's control charts to reflect their patient population's severity of illness during different time intervals. We then demonstrate that risk-adjusting expected patient outcomes can change our assessments of the relative quality of care offered by a health care organization in different time periods.}
}

@article{Alemi2001,
  title = {Tutorial on Risk Adjusted X-Bar Charts: {{Applications}} to Measurement of Diabetes Control},
  author = {Alemi, Farrokh and Sullivan, Thomas},
  year = {2001},
  journal = {Quality Management in Healthcare},
  volume = {9},
  number = {3},
  issn = {1063-8628},
  abstract = {Two data elements are needed for constructing a risk-adjusted X-bar chart. They are a continuous observed outcome collected over time across a sample of patients, and an expected outcome for each patient. The authors selected blood glucose levels to reflect diabetes control. They then reviewed data showing blood glucose levels of 60 Type 2 diabetes patients in a family practice clinic of five providers. The data covered a period of 21 consecutive months. They present the data relating to two of the providers using this set of data to demonstrate how to create a risk-adjusted X-bar control chart. Farrokh Alemi, PhD,is Associate Professor, George Mason University, College of Nursing and Health Science, Fairfax, Virginia. Thomas Sullivan, MD,is Clinical Assistant Professor, Department of Family Practice, Medical College of Virginia, Richmond, Virginia. \textcopyright 2001Aspen Publishers, Inc.},
  keywords = {adjusted performance,diabetes control,physician performance,statistical process control,X-Bar Chart}
}

@article{Alemi2001a,
  title = {Tutorial on Risk-Adjusted p-Charts},
  author = {Alemi, Farrokh and Oliver, Douglas W.},
  year = {2001},
  journal = {Quality Management in Healthcare},
  volume = {10},
  number = {1},
  issn = {1063-8628},
  abstract = {Several books are available that describe how to construct control charts. Unfortunately none of these books show how to adjust control charts for the severity of the patients' illness. Without such adjustments control charts not only loose face validity but could lead to wrong conclusions. In this article we provide a step-by-step guide regarding how to control for changes in patients' severity of illness across time periods. We illustrate the use of the approach in examining falls in nursing homes. A control chart was constructed to compare observed falls to the residents' risk of fall measured on admission. Analysis led to conclusions radically different from an unadjusted chart.},
  keywords = {analysis of risk of falls,probability control chart,process improvement,severity of illness,statistical process control}
}

